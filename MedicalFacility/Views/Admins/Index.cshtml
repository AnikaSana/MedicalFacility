@model IEnumerable<MedicalFacility.Models.Admin>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Admin Name
        </th>
        <th>
            Admin Address
        </th>
        <th>
            Admin contact
        </th>
        <th>
            Admin Email
        </th>
        <th>
            Admin Password
        </th>
        <th></th>
    </tr>

@foreach (var admin in Model) {
    <tr>
        <td>
            @admin.Admin_Name
        </td>
        <td>
            @admin.Admin_Address
        </td>
        <td>
            @admin.Admin_contact
        </td>
        <td>
            @admin.Admin_Email
        </td>
        <td>
            @admin.Admin_Password
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=admin.Admin_ID }) |
            @Html.ActionLink("Details", "Details", new { id=admin.Admin_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=admin.Admin_ID })
        </td>
    </tr>
}

</table>
